{
    "manifest_version": 2,
    "name": "Trapper",
    "description": "",
    "version": "1.0",
    "minimum_chrome_version": "10.0",
    "devtools_page": "trapper.html",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "*://*/*"
    ]
}